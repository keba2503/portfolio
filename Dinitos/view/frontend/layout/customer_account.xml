    <body>
        <block class="Hiperdino\Dinitos\Block\History" name="dinitos_link" template="Hiperdino_Dinitos::history/customer_account_menu_link.phtml" after="-">
            <arguments>
                <argument name="link_position" xsi:type="string">2</argument>
            </arguments>
        </block>
        <block class="Hiperdino\Dinitos\Block\History" name="dinitos_mobile_link" template="Hiperdino_Dinitos::history/customer_account_menu_mobile_link.phtml" before="-">
            <arguments>
                <argument name="link_position" xsi:type="string">2</argument>
            </arguments>
        </block>
        <block name="dinitos_info_modal" class="Hiperdino\Dinitos\Block\History" template="Hiperdino_Dinitos::history/dinitos_info_modal.phtml" after="-"/>
    </body>
</page>